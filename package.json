{
  "name": "Winimoji",
  "version": "1.0.0",
  "description": "A emoji keyboard application",
  "main": "main.js",
  "scripts": {
    "test": "jest --watch",
    "webpack": "webpack --config webpack.config.js --optimize-minimize",
    "electron": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "make-unicodes": "node ./utilities/unicodeUtils.js",
    "make-emojis-babel": "babel --no-babelrc ./utilities/emojiBuilderWithSpread.js --out-file ./utilities/emojiBuilder.js --presets=env,stage-3",
    "make-emojis": "npm run make-emojis-babel && node ./utilities/emojiBuilder.js"
  },
  "build": {
    "appId": "com.saisandeepvaddi.winimoji",
    "compression": "maximum",
    "productName": "Winimoji"
  },
  "repository": "https://github.com/saisandeepvaddi/winimoji",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Sai Sandeep Vaddi <saisandeepvaddi@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.7",
    "devtron": "^1.4.0",
    "electron": "~1.6.2",
    "electron-builder": "^17.10.0",
    "electron-reload": "^1.2.0",
    "enzyme": "2.8.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "20.0.3",
    "jest-cli": "20.0.3",
    "jest-enzyme": "3.1.1",
    "react-addons-test-utils": "15.5.1",
    "react-test-renderer": "15.5.4",
    "regenerator-runtime": "0.10.5",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "bulma": "^0.5.2",
    "electron-settings": "^3.1.1",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.73.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js"
  }
}
